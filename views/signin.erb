<!--SignIn-->
<div class="join_signin_wrap">

  <h1>SignIn</h1>
  <h2>Welcome!</h2>



  <form action="/signin_post" method="post">
    <input class="input" id="email" type="text" name="email" placeholder="Email"><br>
    <input class="input" id="password" type="text" name="password" placeholder="Password"><br>
    <input class="input" id ="signin_button" type="submit" value="SignIn">
  </form>

  <div>
    <h3>
      Not a member?
      <span class="space_between_links"><a href="/registration">Join</a></span>
    </h3>
  </div>

</div>